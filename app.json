{
  "expo": {
    "name": "Drammit",
    "slug": "drammit",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  },
  "infoPlist": {
    "NSPhotoLibraryUsageDescription": "Give Drammit access to your photos to use them in the app.",
    "NSCameraUsageDescription": "Allow Drammit to use your camera."
  }
}
